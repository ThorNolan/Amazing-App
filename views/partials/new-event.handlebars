<div id="modal1" class="modal">
  <div class="modal-content">



    <div class="row">
      <form class="col s12">
        <div class="input-field col s12">
          <input disabled-value="Text" id="eventName" type="text" class="validate">
          <label for="disabled">Name of Event</label>
        </div>
      </form>
    </div>


    <div class="row">
      <form class="col s12">
        <label for="disabled">Date of Event</label>
        <input type="text" class="datepicker" id="eventDate">
      </form>
    </div>


    <div class="row">

      <div class="input-field col s12" id="eventType">
        <select>
          <option value="" disabled selected>Select the Event</option>
          <option value="Birthday">Birthday</option>
          <option value="Anniversary">Anniversary</option>
          <option value="Work Party">Work Party</option>
        </select>
        <label>Event Type</label>
      </div>

    </div>


    <div class="row">
      <form class="col s12">
        <div class="input-field col s12">
          <input disabled-value="Number" id="attendees" type="text" class="validate">
          <label for="disabled">Number of Attendees</label>
        </div>
      </form>
    </div>

    <div class="row">
      <div class="input-field col s12" id="timeOfDay">
        <select>
          <option value="" disabled selected>Time of Day</option>
          <option value="Brunch/Breakfast">Brunch/Breakfast</option>
          <option value="Day Time">Day Time</option>
          <option value="Night Time">Night Time</option>
          <option value="After Hours">After Hours</option>
        </select>
        <label>When time does the party begin?</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s12" id="dress">
        <select>
          <option value="" disabled selected>Attire Type</option>
          <option value="Casual">Casual</option>
          <option value="Semi-Formal">Semi-Formal</option>
          <option value="Formal">Formal</option>
          <option value="Costume/Themed">Costume/Themed</option>
        </select>
        <label>Dress/Clothing Type</label>
      </div>
    </div>


    <div class="row">
      <div class="input-field col s12" id="themed">
        <select>
          <option value="" disabled selected>Is this a themed party?</option>
          <option value="No">No</option>
          <option value="Yes">Yes</option>
        </select>
        <label>Themed - Yes/No</label>
      </div>

    </div>

    <div class="row">
      <div class="input-field col s12" id="food">
        <select>
          <option value="" disabled selected>Do you require food?</option>
          <option value="No">No</option>
          <option value="Yes">Yes</option>
        </select>
        <label>Food Required? - Yes/No</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s12" id="decorations">
        <select>
          <option value="" disabled selected>Do you require decorations?</option>
          <option value="No">No</option>
          <option value="Yes">Yes</option>
        </select>
        <label>Decorations? - Yes/No</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s12" id="cake">
        <select>
          <option value="" disabled selected>Do you need cake/dessert?</option>
          <option value="No">No</option>
          <option value="Yes">Yes</option>
        </select>
        <label>Cake/Dessert? - Yes/No</label>
      </div>
    </div>



    <div class="row">
       <div class="input-field col s12" id="alochol">
        <select>
          <option value="" disabled selected>Do you need alcohol?</option>
          <option value="No">No</option>
          <option value="Yes">Yes</option>
        </select>
        <label>Alochol? - Yes/No</label>
      </div>

    </div>

    <a class="waves-effect waves-light btn-large">Submit</a>


  </div>
</div>